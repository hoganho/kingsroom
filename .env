# CloudWatch Configuration
VITE_CLOUDWATCH_REGION=ap-southeast-2
VITE_CLOUDWATCH_NAMESPACE=ScraperSystem/Client
VITE_METRICS_BATCH_SIZE=20

# .env.development (optional)
VITE_ENABLE_DB_MONITOR=true      # Enable monitoring in dev
VITE_CLOUDWATCH_ENABLED=false    # Maybe disable CloudWatch in dev to save costs

# .env.production (optional)
VITE_ENABLE_DB_MONITOR=false     # Hide monitor UI in production
VITE_CLOUDWATCH_ENABLED=true     # Keep CloudWatch metrics in production

# .env.development (local dev)
VITE_S3_BUCKET=pokerpro-scraper-storage
# .env.production (prod build)
VITE_S3_BUCKET=kingsroom-storage-prod