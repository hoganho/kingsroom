{
  "permissions": {
    "storage": {
      "SocialAccount:@model(appsync)": [
        "create",
        "read",
        "update"
      ],
      "SocialPost:@model(appsync)": [
        "create",
        "read",
        "update"
      ],
      "SocialScrapeAttempt:@model(appsync)": [
        "create",
        "read",
        "update"
      ],
      "SocialScheduledPost:@model(appsync)": [
        "create",
        "read",
        "update"
      ]
    }
  },
  "lambdaLayers": [],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "fbAccessToken",
      "environmentVariableName": "FB_ACCESS_TOKEN"
    },
    {
      "cloudFormationParameterName": "socialPostProcessorFunction",
      "environmentVariableName": "SOCIAL_POST_PROCESSOR_FUNCTION"
    }
  ]
}